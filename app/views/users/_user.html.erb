<li>
  <% if user.oath_image %><%= image_tag(user.oath_image, :style=> "width:50px;height:50px;") %> <% else %> <%= image_tag('avatar-icon.png', :style=>"height:50px;width:50px;") %><% end %><%= link_to user.name, user %> Lifetime Credits: <%= user.lifetime_credits ||= 0 %>


  <% if current_user.admin? && !current_user?(user) %>

    <% if false %><%= link_to "delete", user, method: :delete,
                                  data: { confirm: "You sure?" } %> <% end %>
  <% end %>
</li>
