<% case @survey.slug %>
    <% when "fitness-survey" %>
        <%= render 'survey_one' %>
    <% when "coffee-survey" %>
        <%= render 'survey_two' %>
    <% when "golf-survey" %>
        <%= render 'survey_three' %>
    <% when "movie-survey" %>
        <%= render 'survey_movie' %>
    <% when "oscars-survey" %>
        <%= render 'survey_oscars' %>        
    <% when "travel-survey" %>
        <%= render 'survey_four' %>      
    <% else %>
        <% if @survey.survey_type == "video" %>
            <%= render 'survey_video' %> 
        <% else %>
            <%= render 'survey_new'  %>          
        <% end %>
<% end %> 
